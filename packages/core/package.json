{
  "name": "@vencura/core",
  "version": "0.0.1",
  "private": true,
  "description": "TypeScript SDK for the Vencura API",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "lint": "eslint . --max-warnings 0",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage"
  },
  "dependencies": {
    "@vencura/lib": "workspace:*",
    "@vencura/types": "workspace:*",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^4.0.13",
    "@workspace/eslint-config": "workspace:*",
    "@workspace/typescript-config": "workspace:*",
    "eslint": "^9.32.0",
    "tsup": "^8.3.5",
    "typescript": "^5.9.2",
    "vitest": "^4.0.13"
  }
}
