{
  "name": "@vencura/lib",
  "version": "0.0.1",
  "private": true,
  "description": "Shared utility functions and helpers for Vencura",
  "license": "PROPRIETARY",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/cjs/index.cjs",
      "import": "./dist/esm/index.mjs"
    },
    "./server": {
      "types": "./dist/types/load-env.d.ts",
      "require": "./dist/cjs/load-env.cjs",
      "import": "./dist/esm/load-env.mjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --max-warnings 0 --ignore-pattern 'scripts/**'"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@vencura/types": "workspace:*",
    "lodash-es": "latest",
    "viem": "2.44.1",
    "zod": "^3.25.76",
    "zod-validation-error": "^3.3.0"
  },
  "devDependencies": {
    "@types/lodash-es": "latest",
    "@types/node": "^24.10.1",
    "@workspace/eslint-config": "workspace:*",
    "@workspace/typescript-config": "workspace:*",
    "bun-types": "^1.3.5",
    "eslint": "^9.32.0",
    "tsup": "^8.3.5",
    "typescript": "^5.9.2"
  }
}
