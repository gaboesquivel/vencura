{
  "name": "@vencura/api",
  "version": "0.0.1",
  "private": true,
  "description": "Vencura API built with Elysia",
  "type": "module",
  "main": "./src/index.ts",
  "scripts": {
    "dev": "bun run src/index.ts",
    "build": "tsc --noEmit && bun build src/index.ts --outdir dist --target bun",
    "typecheck": "tsc --noEmit && tsc --noEmit -p tsconfig.spec.json",
    "start": "bun run dist/index.js",
    "lint": "eslint . --max-warnings 0",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage"
  },
  "dependencies": {
    "elysia": "^1.4.21",
    "@elysiajs/openapi": "^1.4.0",
    "@vencura/types": "workspace:*",
    "@vencura/lib": "workspace:*",
    "@vencura/evm": "workspace:*",
    "@dynamic-labs-wallet/node": "^0.0.188",
    "@dynamic-labs-wallet/node-evm": "^0.0.188",
    "@dynamic-labs-wallet/node-svm": "^0.0.188",
    "drizzle-orm": "^0.36.4",
    "@electric-sql/pglite": "^0.2.17",
    "zod": "^3.25.76",
    "viem": "^2.44.1"
  },
  "devDependencies": {
    "bun-types": "^1.3.5",
    "@vitest/coverage-v8": "^4.0.17",
    "@workspace/eslint-config": "workspace:*",
    "@workspace/typescript-config": "workspace:*",
    "eslint": "^9.39.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17"
  }
}
