{
  "name": "@vencura/api",
  "version": "0.0.1",
  "private": true,
  "description": "Vencura API built with Elysia",
  "type": "module",
  "main": "./src/index.ts",
  "scripts": {
    "dev": "bun run src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target bun",
    "start": "bun run dist/index.js",
    "lint": "eslint . --max-warnings 0",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage"
  },
  "dependencies": {
    "elysia": "^1.1.0",
    "@vencura/types": "workspace:*",
    "@vencura/lib": "workspace:*",
    "@dynamic-labs-wallet/node-evm": "^0.0.188",
    "@dynamic-labs-wallet/node-svm": "^0.0.188",
    "drizzle-orm": "^0.36.4",
    "@electric-sql/pglite": "^0.2.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@vitest/coverage-v8": "^4.0.13",
    "@workspace/eslint-config": "workspace:*",
    "@workspace/typescript-config": "workspace:*",
    "eslint": "^9.32.0",
    "typescript": "^5.9.2",
    "vitest": "^4.0.13"
  }
}
