# Environment Configuration Template for Sensitive Data
# Copy this file to .env and fill in your actual values
# NEVER commit .env file - it contains sensitive data

# Required: Dynamic SDK credentials
DYNAMIC_ENVIRONMENT_ID=your_environment_id_here
DYNAMIC_API_TOKEN=your_api_token_here

# Required: Encryption key (minimum 32 characters)
ENCRYPTION_KEY=your_32_character_encryption_key_here_minimum_length

# Required for tests: Arbitrum Sepolia gas faucet private key
# Used to fund test wallets with minimum ETH required for transactions
ARB_TESTNET_GAS_FAUCET_KEY=your_arbitrum_sepolia_faucet_private_key_here

# Optional: Testing - Pre-configured JWT token for testing
# In test mode (NODE_ENV=test), DYNAMIC_API_TOKEN is used directly for authentication.
# Only set this if you want to use a specific JWT token instead of the API key.
# TEST_AUTH_TOKEN=your_jwt_token_from_dynamic_here

# Optional: RPC URL overrides for specific chains
# RPC_URL_421614=https://sepolia-rollup.arbitrum.io/rpc

# Optional: AI/LLM configuration
# OPEN_AI_KEY=your_openai_key_here

# Optional: Sentry error tracking
# SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id
# SENTRY_ENVIRONMENT=development

