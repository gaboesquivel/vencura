{
  "name": "dynamic",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "hooks:pre-commit": "pnpm lint && pnpm format",
    "qa": "pnpm i && pnpm run format && turbo run lint && turbo build && turbo run test --continue",
    "contracts:evm:build": "cd contracts/evm && forge build",
    "contracts:evm:test": "cd contracts/evm && forge test",
    "contracts:solana:build": "cd contracts/solana && anchor build",
    "contracts:solana:test": "cd contracts/solana && anchor test"
  },
  "devDependencies": {
    "@total-typescript/ts-reset": "^0.6.1",
    "@workspace/eslint-config": "workspace:*",
    "@workspace/typescript-config": "workspace:*",
    "prettier": "^3.6.2",
    "prettier-plugin-solidity": "^2.2.0",
    "turbo": "^2.6.1",
    "typescript": "5.7.3"
  },
  "packageManager": "pnpm@10.4.1",
  "engines": {
    "node": ">=20"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@nestjs/core",
      "@swc/core"
    ],
    "overrides": {
      "@dynamic-labs/wallet-connector-core": "4.45.0"
    }
  }
}